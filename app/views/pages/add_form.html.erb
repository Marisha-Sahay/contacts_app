<%= form_tag '/form_result', method: :post do %>

  <div>
    <%= label_tag :contact, "First name" %>
    <%= text_field_tag :contact %>
  </div>
  <div>
    <%= label_tag :phone_no, "Phone number" %>
    <%= text_field_tag :phone_no %>
  </div>
  <div>
    <%= submit_tag "submit" %>    
  </div>

<% end %>
